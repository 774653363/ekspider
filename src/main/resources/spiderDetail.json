          "exp":"putUrl##concatUrl#http://www.abchina.com/cn/businesses/FinancialMarket/jrscrb/##downloadUrl"
          },
            "detailOperates": [
              {
                "exp":"regex#foreach#downloadName#金融市场日报[0-9-]+"
              },{
                "exp": "download#concatUrl###downloadName#F:\\download\\pdf"
              }
            ]
          }
        }
  ]
}

